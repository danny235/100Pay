{
  "expo": {
    "name": "hundredpay",
    "slug": "hundredpay",
    "version": "1.0.0",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SpaceGroteskBold.ttf",
            "./assets/fonts/SpaceGroteskLight.ttf",
            "./assets/fonts/SpaceGroteskRegular.ttf",
            "./assets/fonts/SpaceGroteskMedium.ttf",
            "./assets/fonts/SpaceGroteskSemiBold.ttf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-router"
    ],
    "web": {
      "output": "single",
      "bundler": "metro"
    }
  }
}
